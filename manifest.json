{
    "name": "Instergram anonymously",
    "description": "This extension is made for anonymously your instergram traffic",
    "version": "1.0",
    "manifest_version": 3,
    "action": {},
    "permissions": [
         "activeTab",
         "scripting",
         "webRequest",
         "tabs"
         
    ],
    "host_permissions": [
    "http://*/*",   
    "https://*/*"
  ],
    "background": {
       "service_worker": "./dist/background.js"
    }
 }